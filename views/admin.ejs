
<!DOCTYPE html>
<html>
	<%- include("partials/head") %>
<body>

	<%- include("./partials/adminHeader.ejs") %>
	<div class="user-container">
			<div class="d-flex align-center space">
				<h3 class="text">Welcome <%= user.username%></h3>
			</div>
			

			<div class="table-wrapper">
				<table class="table">
					<caption>User Profile</caption>
					<tr>
						<th>ID</th>
						<th>Username</th>
						<th>Role</th>
						<th>Picture</th>
					</tr>

					<tr>
						<td><%= user._id%></td>
						<td><%= user.username%></td>
						<td><%= user.role%></td>
						<td><img src="/assets/uploads/<%= user.picture%>" class="user"></td>
					</tr>
				</table>				
			</div>

	</div>
		
</body>
</html>